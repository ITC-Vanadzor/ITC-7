#include <iostream>

int Factorial(int N)
{
    return (N == 1 ? N : N * Factorial(N - 1));
}

int main()
{
    int N;
    int K;
    std::cout << "N x N = " ;
    std::cin >> N;
    std::cout << "K = ";
    std::cin>> K;

    std::cout << "Kareli e dasavorel " << (Factorial(N) * Factorial(N)) / Factorial(K) << " exanakov  " << std::endl;
    return 0;
}
